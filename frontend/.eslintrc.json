{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "indent": ["error", 4],
      "no-console": "off",
      "comma-dangle": "off",
      "react/jsx-filename-extension": "off",
      "react/jsx-indent": "off",
      "max-len": ["error", { "code": 100, "tabWidth": 4 }],
      "no-underscore-dangle": "off",
      "react/forbid-prop-types": "off",
      "no-shadow": "off",
      "react/jsx-indent-props": "off",
      "react/require-default-props": "off"
    }
  }